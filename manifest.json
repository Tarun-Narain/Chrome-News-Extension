{
  "manifest_version": 2,
  "name": "NEWS Feed ",
  "version": "1.2",

"content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["jquery-3.5.1.js" , "content.js"]
  }
],

"browser_action": {
  "default_icon": "icon.png",
"default_popup" : "shaista_content.html"
},

"background": {
	"scripts" : ["jquery-3.5.1.js","background.js"]
},
"content_security_policy": "script-src 'self' https://www.google.com;object-src 'self'",
"permissions":[
"activeTab"
]
}
